void screenScroll(int scrollLevel) {
    //The if statement is for button. Do not use with potentiometer.    
    lcd.clear();
    lcd.setCursor(0, 0);
    lcd.print(scrollLevel + 1);
    lcd.setCursor( (16 - songNames[scrollLevel][0].length() ) / 2 , 0);
    lcd.print(songNames[scrollLevel][0]);
    lcd.setCursor( (16 - songNames[scrollLevel][1].length() ) / 2 , 1);
    lcd.print(songNames[scrollLevel][1]);
    if(song_last_played == scrollLevel){
      changePlayButton(0);
    }
    }
    
void changePlayButton(int state){
  lcd.setCursor(15,0);
  if (state == 0){
    lcd.write(byte(0)); //Pause Button
    }
  else if(state == 1){
    lcd.write(byte(1)); //Play Button
  }
}

void changeSpeakerOutput(int pinNo){
  currentSpeakerPin = pinNo;
}

void pollSwitchButton(){
  if (checkSwitchSelect() == true) {
    noTone(currentSpeakerPin);
    if (currentSpeakerPin == speakerPin){
      currentSpeakerPin = altPin;
      Serial.println("Switching to altpin...");
      Serial.println("Current speaker pin: " + String(currentSpeakerPin));
      Serial.println("altPin value: " + String(altPin));
    }
    else {
      currentSpeakerPin = speakerPin;
      Serial.println("Switching to main speaker pin...");
      Serial.println("Current speaker pin: " + String(currentSpeakerPin));
    }
  }
}

void shuffle(){
  shuffleDone = false;
  while (true){
    int randomVariable = randomNumber(true);
    int lastSongPlayed;
    while(randomVariable == lastSongPlayed){
      randomVariable = randomNumber(true);
    }
    lastSongPlayed = randomVariable;
    screenScroll(lastSongPlayed);
    lcd.setCursor(15,1);
    lcd.write(byte(2));
    changePlayButton(1);
    delay(750);
    play_song(songList[randomVariable],true);
    if (shuffleDone = true){
      screenScroll(maxSong - 1);
      break;    
    }
  }
}

int randomNumber(bool ifShuffle){
  if(ifShuffle == true){
    int randomVariable = random(0,maxSong - 1);
    return randomVariable;
  }
  else{
    int randomVariable = random(0,256);
    return randomVariable;
  }
}

void random_screen_color(){
  lcd.setRGB(randomNumber(false),randomNumber(false),randomNumber(false));
}

void setup() {
  Serial.begin(9600);
  pinMode(potPin,INPUT);
  pinMode(buttonPin, INPUT);
  pinMode(switchButtonPin, INPUT);
  pinMode(speakerPin, OUTPUT);
  pinMode(altPin, OUTPUT);
  lcd.begin(16, 2);
  lcd.createChar(0, pauseButton);
  lcd.createChar(1, playButton);
  lcd.createChar(2, shuffleSprite);
  randomSeed(analogRead(random_noise_pin));
  screenScroll(checkPot_and_scrollLevel());
 }


void loop() {
  //Checks to see if button is not pressed.
  if (checkSelect() == false) {
    //Next few lines check to see if you scrolled to a different song.
    scrollSwitch[0] = scrollSwitch[1];
    scrollSwitch[1] = checkPot_and_scrollLevel();
    if (scrollSwitch[0] != scrollSwitch[1]) {
      screenScroll(checkPot_and_scrollLevel());
    }
    pollSwitchButton();

  } else {
      changePlayButton(1);
      if(scrollLevel == maxSong - 1){
        Serial.println("Shuffle Started");
        shuffle();
    }
     else{
        play_song(songList[scrollLevel],false);
    }
  }
}
