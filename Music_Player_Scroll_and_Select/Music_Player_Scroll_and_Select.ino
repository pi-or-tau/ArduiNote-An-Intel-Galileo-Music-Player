//Time Variables
unsigned long currentTime;
unsigned long time_since_last_press = 0;
const int debounceTime = 250;                  //Milliseconds of buffer time

//Sensors
int potPosition;
bool buttonPosition;
//bool buttonTrue;

//Play+Pause
bool pauseTrue;
bool playTrue;

/*TODO
buttonState = changePlayButton(buttonState);

